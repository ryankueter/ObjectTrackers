<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net5.0;net6.0</TargetFrameworks>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>1.0.18</Version>
		<Authors>Ryan A. Kueter</Authors>
		<Company>Ryan A. Kueter</Company>
		<Description>A free .NET library that provides a simple way to track changes made to an object, like a class or list of classes. It allows you to capture those changes as “before” and “after” json serialized dictionary arrays that contain the property names and their values for quickly and easily storing and retrieving your audit data.</Description>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageIcon>Icon.jpg</PackageIcon>
		<PackageIconUrl />
		<RepositoryUrl>https://github.com/ryankueter/ObjectTrackers</RepositoryUrl>
		<PackageReleaseNotes>Removed System.Text.Encodings.Web.JavaScriptEncoder.UnsafeRelaxedJsonEscaping.</PackageReleaseNotes>
		<PackageTags>C#;Track;Tracking;Changes;Auditing</PackageTags>
		<AssemblyVersion>1.0.18</AssemblyVersion>
		<FileVersion>1.0.18</FileVersion>
		<Copyright>Ryan A. Kueter</Copyright>
		<PackageProjectUrl>https://github.com/ryankueter/ObjectTrackers</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryType>Git</RepositoryType>
		<Product>$(AssemblyName)</Product>
		<NeutralLanguage>en</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Icon.jpg">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

</Project>
